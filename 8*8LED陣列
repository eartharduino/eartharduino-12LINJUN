#include <LedControl.h>
LedControl lc=LedControl(12,11,10,1);
void setup(){
  lc.shutdown(0,false);
  lc.setIntensity(0,8);
  lc.clearDisplay(0);
}
void loop(){
lc.setRow(0,0,B11111111);
lc.setRow(0,1,B11111111);
lc.setRow(0,2,B00011000);
lc.setRow(0,3,B00011000);
lc.setRow(0,4,B00011000);
lc.setRow(0,5,B00011000);
lc.setRow(0,6,B11111111);
lc.setRow(0,7,B11111111);
delay(500);
lc.setRow(0,0,B11000011);
lc.setRow(0,1,B11000011);
lc.setRow(0,2,B11000011);
lc.setRow(0,3,B11111111);
lc.setRow(0,4,B11111111);
lc.setRow(0,5,B11000011);
lc.setRow(0,6,B11000011);
lc.setRow(0,7,B11000011);
delay(500);
lc.setRow(0,0,B00111100);
lc.setRow(0,1,B00100100);
lc.setRow(0,2,B00100100);
lc.setRow(0,3,B00111100);
lc.setRow(0,4,B00100100);
lc.setRow(0,5,B00100100);
lc.setRow(0,6,B00100100);
lc.setRow(0,7,B00100100);
delay(500);
lc.setRow(0,0,B00000000);
lc.setRow(0,1,B00000000);
lc.setRow(0,2,B10000001);
lc.setRow(0,3,B01000010);
lc.setRow(0,4,B00100100);
lc.setRow(0,5,B00011000);
lc.setRow(0,6,B00000000);
lc.setRow(0,7,B00000000);
delay(500);
lc.setRow(0,0,B00011000);
lc.setRow(0,1,B00100100);
lc.setRow(0,2,B01000010);
lc.setRow(0,3,B11111111);
lc.setRow(0,4,B10000000);
lc.setRow(0,5,B10000001);
lc.setRow(0,6,B01000010);
lc.setRow(0,7,B00111100);
delay(500);
lc.setRow(0,0,B00111100);
lc.setRow(0,1,B01000010);
lc.setRow(0,2,B10100101);
lc.setRow(0,3,B10000001);
lc.setRow(0,4,B10100101);
lc.setRow(0,5,B10011001);
lc.setRow(0,6,B01000010);
lc.setRow(0,7,B00111100);
delay(500);
}
